#!/bin/sh

output() {
  retcode=$?
  if [ $retcode -eq 0 ]
    then echo "PASS: $i"
  elif [ $retcode -eq 77 ]
    then echo "SKIP: $i"
  else echo "FAIL: $i"
  fi
}

export DBUS_TEST_HOMEDIR=./test
export XDG_RUNTIME_DIR=./test
export LD_LIBRARY_PATH=@PTEST_PATH@/test/.libs

files=`ls test/test-*`

for i in $files
	do
		./$i ./test/data >/dev/null
		output
	done

