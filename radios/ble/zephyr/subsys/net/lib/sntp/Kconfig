menuconfig SNTP
	bool "SNTP Support"
	default n
	select NET_APP_CLIENT
	help
	  Enable SNTP client library

if SNTP

config NET_DEBUG_SNTP
	bool "Debug SNTP"
	default n
	help
	  Enable debug message of SNTP client library

endif # SNTP
