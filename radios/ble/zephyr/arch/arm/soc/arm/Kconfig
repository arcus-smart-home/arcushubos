config SOC_FAMILY_ARM
	bool
	# omit prompt to signify a "hidden" option
	default n

if SOC_FAMILY_ARM
config SOC_FAMILY
	string
	default "arm"

gsource "arch/arm/soc/arm/*/Kconfig.soc"
endif # SOC_FAMILY_ARM
