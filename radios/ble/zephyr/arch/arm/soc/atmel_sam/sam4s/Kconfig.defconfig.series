if SOC_SERIES_SAM4S

config SOC_SERIES
	string
	default "sam4s"

config SOC_PART_NUMBER
	string
	default "sam4s16c" if SOC_PART_NUMBER_SAM4S16C

#
# SAM4S family has total 35 peripherals capable of
# generating interrupts.
#
config NUM_IRQS
	int
	default 35

config SYS_CLOCK_HW_CYCLES_PER_SEC
	int
	default 120000000

endif # SOC_SERIES_SAM4S
